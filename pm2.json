{
  "name": "nebula",
  "exec_mode": "cluster",
  "instances": "max",
  "script": "./node_modules/nuxt/bin/nuxt.js",
  "args": "start",
  "env": {
    "NODE_ENV": "production",
    "VUE_APP_TITLE": "production"
  },
  "env_dev": {
    "NODE_ENV": "development",
    "VUE_APP_TITLE": "development"
  },
  "env_prod": {
    "NODE_ENV": "production",
    "VUE_APP_TITLE": "production"
  }
}
